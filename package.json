    "dotenv": "^16.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
